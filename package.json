{
  "name": "rack-research",
  "version": "1.0.0",
  "description": "Rack mount server chassis research and product catalog scraper",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev --prefix website",
    "build": "npm run build --prefix website",
    "start": "npm run start --prefix website",
    "scrape:yakkaroo": "node pipeline/scrapers/yakkaroo/1-download-html.js && node pipeline/scrapers/yakkaroo/2-parse-specs.js",
    "scrape:yakkaroo:download": "node pipeline/scrapers/yakkaroo/1-download-html.js",
    "scrape:yakkaroo:parse": "node pipeline/scrapers/yakkaroo/2-parse-specs.js",
    "build:db": "tsx --tsconfig pipeline/tsconfig.json pipeline/src/build-db.ts",
    "extract:urls": "tsx --tsconfig pipeline/tsconfig.json pipeline/scripts/extract-urls.ts",
    "verify:urls": "tsx --tsconfig pipeline/tsconfig.json pipeline/scripts/verify-urls.ts",
    "report:quality": "tsx --tsconfig pipeline/tsconfig.json pipeline/scripts/report-data-quality.ts",
    "typecheck": "tsc --project pipeline/tsconfig.json --noEmit",
    "typecheck:web": "tsc --project website/tsconfig.json --noEmit",
    "lint": "eslint pipeline/src/ pipeline/scrapers/",
    "lint:fix": "eslint pipeline/src/ pipeline/scrapers/ --fix",
    "format": "prettier --write 'pipeline/src/**/*.ts'",
    "format:check": "prettier --check 'pipeline/src/**/*.ts'",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:e2e": "npm run test:e2e --prefix website"
  },
  "keywords": [
    "rack-mount",
    "server-chassis",
    "scraper",
    "database"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.1.2",
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "npm@10.9.2"
}
