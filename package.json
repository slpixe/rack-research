{
  "name": "rack-research",
  "version": "1.0.0",
  "description": "Rack mount server chassis research and product catalog scraper",
  "private": true,
  "type": "module",
  "scripts": {
    "scrape:yakkaroo": "node scrapers/yakkaroo/1-download-html.js && node scrapers/yakkaroo/2-parse-specs.js",
    "scrape:yakkaroo:download": "node scrapers/yakkaroo/1-download-html.js",
    "scrape:yakkaroo:parse": "node scrapers/yakkaroo/2-parse-specs.js",
    "build:db": "tsx src/build-db.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "keywords": [
    "rack-mount",
    "server-chassis",
    "scraper",
    "database"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/ui": "^4.0.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
